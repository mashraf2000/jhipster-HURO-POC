<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity EntitySeekingFund.
    -->
    <changeSet id="20200115051524-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="entity_seeking_fund_id"
                                 baseTableName="entity_seeking_fund_intent"
                                 constraintName="fk_entity_seeking_fund_intent_entity_seeking_fund_id"
                                 referencedColumnNames="id"
